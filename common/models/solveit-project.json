{
  "name": "Solveitproject",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "proposal": {
      "type": "object",
      "required": false
    },
    "bussinessPlan": {
      "type": "object"
    },
    "showcased": {
      "type": "boolean",
      "default": false
    },
    "createdAt": {
      "type": "date",
      "defaultfun": "now"
    },
    "score": {
      "type": "array",
      "default": []
    },
    "unregisteredMembers": {
      "type": "array",
      "default": []
    }
  },
  "validations": [],
  "relations": {
    "members": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "projectId",
      "through": "project-member"
    },
    "reports": {
      "type": "hasMany",
      "model": "ProgressReport",
      "foreignKey": "projectId"
    },
    "ratings": {
      "type": "hasMany",
      "model": "projectRating",
      "foreignKey": "projectId"
    },
    "competitions": {
      "type": "hasMany",
      "model": "solvieITCompetition",
      "foreignKey": "projectId",
      "through": "CompetitionProject"
    },
    "bookmarkers": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "projectId",
      "through": "projectBookmark"
    },
    "viewers": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "projectId",
      "through": "projectView"
    },
    "won": {
      "type": "hasMany",
      "model": "solvieITCompetition",
      "foreignKey": "projectId",
      "through": "competitionWinner"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY",
      "property": "upsert"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "upsert"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY",
      "property": "updateAttributes"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "updateAttributes"
    }
  ],
  "methods": {}
}
