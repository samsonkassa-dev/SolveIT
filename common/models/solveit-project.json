{
  "name": "Solveitproject",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "proposal": {
      "type": "object",
      "required": true
    },
    "showcased": {
      "type": "boolean",
      "default": false
    },
    "createdAt": {
      "type": "date",
      "defaultfun": "now"
    }
  },
  "validations": [],
  "relations": {
    "members": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "projectId",
      "through": "project-member"
    },
    "reports": {
      "type": "hasMany",
      "model": "ProgressReport",
      "foreignKey": "projectId"
    },
    "competitions": {
      "type": "hasMany",
      "model": "solvieITCompetition",
      "foreignKey": "projectId",
      "through": "CompetitionProject"
    }
  },
  "acls": [],
  "methods": {}
}
