{
  "name": "SolveITForum",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "slung": {
      "type": "string",
      "required": true
    },
    "private": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "discussionCount": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "discussions": {
      "type": "hasMany",
      "model": "SolveIT-Discussion",
      "foreignKey": "forumId"
    },
    "category": {
      "type": "belongsTo",
      "model": "forumCategory"
    },
    "members": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "forumId",
      "through": "forum_member"
    }
  },
  "acls": [],
  "methods": {}
}
