{
  "name": "solvieITCompetition",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "active": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "startingDate": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "projects": {
      "type": "hasMany",
      "model": "Solveitproject",
      "foreignKey": "competitionId",
      "through": "CompetitionProject"
    },
    "winner": {
      "type": "hasMany",
      "model": "Solveitproject",
      "foreignKey": "competitionId",
      "through": "competitionWinner"
    }
  },
  "acls": [],
  "methods": {}
}
