{
  "name": "judge",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "approved": {
      "type": "boolean",
      "default": false
    },
    "competitions": {
      "type": ["string"]
    }
  },
  "validations": [],
  "relations": {
    "judge": {
      "type": "belongsTo",
      "model": "UserAccount",
      "foreignKey": "judgeId"
    },
    "role": {
      "type": "belongsTo",
      "model": "Icog-Role",
      "foreignKey": "roleId"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "participant",
      "permission": "DENY",
      "property": "*"
    }
  ],
  "methods": {}
}
