{
  "name": "UserAccount",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "firstName": {
      "type": "string",
      "required": true,
      "default": "None"
    },
    "middleName": {
      "type": "string",
      "required": true,
      "default": "None"
    },
    "lastName": {
      "type": "string",
      "required": true,
      "default": "None"
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "default": "None"
    },
    "rememberMe": {
      "type": "boolean"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "ACTIVE"
    },
    "sex": {
      "type": "string",
      "required": "true",
      "default": "None"
    },
    "age": {
      "type": "number",
      "required": "true",
      "default": "None"
    },
    "educationLevel": {
      "type": "string",
      "required": "true",
      "default": "None"
    },
    "workStatus": {
      "type": "string",
      "required": "true",
      "default": "None"
    },
    "address": {
      "type": "object",
      "required": "true",
      "default": "None"
    },
    "emailVerified": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "role": {
      "type": "belongsTo",
      "model": "Icog-Role",
      "foreignKey": "roleId"
    },
    "region": {
      "type": "belongsTo",
      "model": "Region",
      "foreignKey": "regionId"
    },
    "solveIT-discussions": {
      "type": "hasMany",
      "model": "Solveitdiscussion",
      "foriegnKey": "userAccountId"
    },
    "forums": {
      "type": "hasMany",
      "model": "SolveITForum",
      "foreignKey": "userId",
      "through": "forum_member"
    },
    "favouriteDiscussions": {
      "type": "hasMany",
      "model": "Solveitdiscussion",
      "foreignKey": "userId",
      "through": "Favouritediscussion"
    },
    "projects": {
      "type": "hasMany",
      "model": "Solveitproject",
      "foreignKey": "userId",
      "through": "project-member"
    },
    "blackListed": {
      "type": "hasMany",
      "model": "Solveitdiscussion",
      "foreignKey": "userId",
      "through": "BlackListedDiscussion"
    },
    "progressComments": {
      "type": "hasMany",
      "model": "progressComment",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
