{
  "name": "Solveitdiscussion",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "slung": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "content": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": "true"
    },
    "imgContent": {
      "type": "object"
    },
    "pinned": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "comments": {
      "type": "hasMany",
      "model": "SolveIT-Discussion-Comment",
      "foriegnKey": "discussionId"
    },
    "favouriteUsers": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "discussionId",
      "through": "Favouritediscussion"
    },
    "user": {
      "type": "belongsTo",
      "model": "UserAccount",
      "foreignKey": "userAccountId"
    },
    "blackListed": {
      "type": "hasMany",
      "model": "progressComment",
      "foreignKey": "discussionId",
      "through": "BlackListedDiscussion"
    },
    "tags": {
      "type": "hasMany",
      "model": "tag",
      "foreignKey": "discussionId",
      "through": "taggedDiscussion"
    }
  },
  "acls": [],
  "methods": {}
}
