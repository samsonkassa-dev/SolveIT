{
  "name": "Solveitdiscussion",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "slung": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "content": {
      "type": "string",
      "required": true
    },
    "pinned": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "comments": {
      "type": "hasMany",
      "model": "SolveIT-Discussion-Comment",
      "foriegnKey": "discussionId"
    },
    "favouriteUsers": {
      "type": "hasMany",
      "model": "UserAccount",
      "foreignKey": "discussionId",
      "through": "Favouritediscussion"
    }
  },
  "acls": [],
  "methods": {}
}
